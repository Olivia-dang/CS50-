#include <stdio.h>
#include <cs50.h>
float average(int length, int array[]);
int main(void)
{
    int scores[3];
    for (int i=0; i<3; i++)
    {
        scores[i]= get_int("Score: ");
    }
    printf("Average: %f\n ", average(3, scores));
}    
    float average(int length, int array[])
{
    int sum =0;
    for (int i=0; i<length; i++)
    {
        sum+= array[i];
    }
    return sum/(float) length;
}
